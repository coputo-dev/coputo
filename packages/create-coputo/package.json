{
  "publishConfig": {
    "access": "public"
  },
  "name": "create-coputo",
  "version": "0.1.7",
  "type": "module",
  "files": ["dist", "templates"],
  "bin": {
    "create-coputo": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "tsup src/index.ts --watch --dts --format esm",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "lint": "biome check src --apply",
    "format": "biome format . --write",
    "prepare": "pnpm run build"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@coputo/core": "workspace:*",
    "@coputo/fastify": "workspace:*",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9"
  }
}
